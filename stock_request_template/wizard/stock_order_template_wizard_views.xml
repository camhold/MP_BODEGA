<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="template_order_wizard_tree" model="ir.ui.view">
        <field name="name">stock.order.template.wizard</field>
        <field name="model">stock.order.template.wizard</field>
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <tree create="false">
                <field name="order_id" invisible="1"/>
                <field name="template_id"/>
                <field name="code"/>
                <field name="description"/>
                <button name="button_add_template"
                        type="object" string="Usar plantilla"
                        class="btn-primary"/>
            </tree>
        </field>
    </record>

</odoo>